_run ()
{
    ! rg -qw 'archboot' /etc/hostname && return
    _install_files
    rm /usr/bin/{3cpio,blkid,gzip,journalctl,less,lsmod,mkfs.btrfs,\
mountpoint,setfont,systemctl,sulogin}
}

